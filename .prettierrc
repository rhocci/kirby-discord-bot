{
	"semi": true,
	"singleQuote": true,
	"printWidth": 80,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^discord\\.js$",
		"^@supabase/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true
}
